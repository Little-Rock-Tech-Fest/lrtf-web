extends ../layouts/default

block intro

	#isNotFront
		section.hero.is-primary
			.hero-body
				.container
					h1.title Past Events


block content

	.container
		.columns.is-multiline.m-t-32.m-b-32
			each year in data.years
				.column.is-one-third
					a(href='/events/'+year)
						.card
							.card-image
								figure.image.is-4by3
									img(src="https://bulma.io/images/placeholders/1280x960.png")
							.card-footer
								.card-footer-item.has-text-primary #[b !{year}]